{
  "manifest_version": 3,
  "name": "NIL (Natural Interaction Layer)",
  "version": "1.0",
  "description": "A Chrome extension that uses DeepSeek R1 model through Ollama",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:11434/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon01.png",
      "48": "icons/icon01.png",
      "128": "icons/icon01.png"
    }
  },
  "icons": {
    "16": "icons/icon01.png",
    "48": "icons/icon01.png",
    "128": "icons/icon01.png"
  }
} 